# Informint Environment Configuration
# Copy this file to .env and fill in your actual values

# Apify Configuration (Required)
APIFY_TOKEN=

# Optional: API endpoint if you ever fork it
APIFY_ACTOR_ID=

APIFY_TASK_ID=


VERCEL_TOKEN=
VERCEL_ORG_ID=
# Server Configuration (Optional)
PORT=3000
BASE_URL=http://localhost:3000

# Supabase Configuration (Required for database functionality)
NEON_URL=your_supabase_project_url_here
NEON_ANON_KEY=your_supabase_anon_key_here
NEON_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
NEON_PROJECT_REF=your_supabase_project_ref_here

# Logging Configuration (Optional)
LOG_LEVEL=info

# CORS Configuration (Optional)
CORS_ORIGIN=* 